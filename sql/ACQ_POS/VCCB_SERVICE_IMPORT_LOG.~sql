CREATE TABLE VCCB_SERVICE_IMPORT_LOG 
(
       FILE_ID NUMBER(20),
       IMP_NAME VARCHAR2(250),
       NUM_OF_RECORD NUMBER(18),
       TEMPLATE_NAME VARCHAR2(250),
       FILE_NAME VARCHAR2(250),
       FILE_TYPE VARCHAR2(10),
       FILE_PATH VARCHAR2(250),
       NOTE VARCHAR2(250),
	   ACTIVE	NUMBER(1),
	   REC_CREATED_DT           DATE,
	   REC_UPDATED_DT           DATE,
	   IMPORT_DATE DATE
       
)
